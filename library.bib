@article{Chu:2014gy,
author = {Chu, Bonnie and Chui, Vincent and Mann, Kevin and Gordon, Michael D},
title = {{Presynaptic Gain Control Drives Sweet and Bitter Taste Integration in Drosophila}},
journal = {Current biology : CB},
year = {2014},
volume = {24},
number = {17},
pages = {1978--1984},
month = aug,
publisher = {Elsevier},
affiliation = {Department of Zoology, Cell and Developmental Biology, University of British Columbia, Vancouver, BC V6T 1Z3, Canada.},
keywords = {Drosophila, gain-control, gustation},
doi = {10.1016/j.cub.2014.07.020},
pmid = {25131672},
language = {English},
rating = {0},
date-added = {2014-12-03T22:01:47GMT},
date-modified = {2017-03-14T21:18:49GMT},
abstract = {The sense of taste is critical in determining the nutritional suitability of foods. Sweet and bitter are primary taste modalities in mammals, and their behavioral relevance is similar in flies. Sweet taste drives the appetitive response to energy sources, whereas bitter taste drives avoidance of potential toxins and also suppresses the sweet response [1, 2]. Despite their importance to survival, little is known about the neural circuit mechanisms underlying integration of sweet and bitter taste. Here, we describe a presynaptic gain control mechanism in Drosophila that differentially affects sweet and bitter taste channels and mediates integration of these opposing stimuli. Gain control is known to play an important role in fly olfaction, where GABAB receptor (GABABR) mediates intra- and interglomerular presynaptic inhibition of sensory neuron output [3-5]. In the taste system, we find that gustatory receptor neurons (GRNs) responding to sweet compounds express GABABR, whereas those that respond to bitter do not. GABABR mediates presynaptic inhibition of calcium responses in sweet GRNs, and both sweet and bitter stimuli evoke GABAergic neuron activity in the vicinity of GRN axon terminals. Pharmacological blockade and genetic reduction of GABABR both lead to~increased sugar responses and decreased suppression of the sweet response by bitter compounds. We propose a~model in which GABA acts via GABABR to expand the dynamic range of sweet GRNs through presynaptic gain control and suppress the output of sweet GRNs in the presence of opposing bitter stimuli.},
url = {http://www.cell.com/article/S096098221400846X/fulltext},
local-url = {file://localhost/Users/sigbhu/Dropbox/Library.papers3/Articles/2014/Chu/Current%20Biology%202014%20Chu.pdf},
file = {{Current Biology 2014 Chu.pdf:/Users/sigbhu/Dropbox/Library.papers3/Articles/2014/Chu/Current Biology 2014 Chu.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.cub.2014.07.020}}
}

