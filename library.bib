affiliation = {Program for Evolutionary Dynamics, Department of Psychology, Society of Fellows, and Departments of Organismic and Evolutionary Biology and Mathematics, Harvard University, Cambridge, MA 02139.},
keywords = {cooperation, evolution, sociodynamics, sociology},
doi = {10.1073/pnas.1214167110},
pmid = {23341593},
language = {English},
read = {Yes},
