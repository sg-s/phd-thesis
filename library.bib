title = {{How Multiple Conductances Determine Electrophysiological Properties in a Multicompartment Model}},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
year = {2009},
volume = {29},
number = {17},
pages = {5573--5586},
month = apr,
publisher = {Society for Neuroscience},
affiliation = {Volen Center, Brandeis University, Waltham, Massachusetts 02454, USA. altaylor@brandeis.edu},
keywords = {computational-neuroscience, neuron-model, STG},
doi = {10.1523/JNEUROSCI.4438-08.2009},
