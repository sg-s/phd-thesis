@article{Guerrieri:2005gx,
author = {Guerrieri, Fernando and Schubert, Marco and Sandoz, Jean-Christophe and Giurfa, Martin},
title = {{Perceptual and neural olfactory similarity in honeybees.}},
journal = {PLoS Biology},
year = {2005},
volume = {3},
number = {4},
pages = {e60},
month = apr,
affiliation = {Centre de Recherches sur la Cognition Animale, CNRS, Universit{\'e} Paul-Sabatier (UMR 5169), Toulouse, France.},
