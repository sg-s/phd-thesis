pages = {12325--12330},
month = aug,
publisher = {National Acad Sciences},
affiliation = {Institute of Computing Technology, Chinese Academy of Sciences, Beijing 100190, China; Center for Complex Network Research and Departments of Physics, Biology, and Computer Science, Northeastern University, Boston, MA 02115;},
keywords = {collaboration, cooperation, meta-science, networks},
