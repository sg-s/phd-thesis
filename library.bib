title = {{Models in biology: {\textquoteleft}accurate descriptions of our pathetic thinking{\textquoteright}}},
journal = {BMC Biology},
year = {2014},
volume = {12},
number = {1},
pages = {29},
month = apr,
publisher = {BioMed Central Ltd},
affiliation = {Department of Systems Biology, Harvard Medical School, 200 Longwood Avenue, Boston, USA. jeremy@hms.harvard.edu.},
