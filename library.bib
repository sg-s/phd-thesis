@article{Petersen:2010jw,
author = {Petersen, Alexander M and Wang, Fengzhong and Havlin, Shlomo and Stanley, H Eugene},
title = {{Market dynamics immediately before and after financial shocks: quantifying the Omori, productivity and Bath laws}},
journal = {arXiv.org},
year = {2010},
eprint = {1006.1882v2},
