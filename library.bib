file = {{E1FCD49F-5C0C-46BA-8993-D74383FDFC4C.pdf:/Users/sigbhu/Dropbox/Library.papers3/Files/E1/E1FCD49F-5C0C-46BA-8993-D74383FDFC4C.pdf:application/pdf;E1FCD49F-5C0C-46BA-8993-D74383FDFC4C.pdf:/Users/sigbhu/Dropbox/Library.papers3/Files/E1/E1FCD49F-5C0C-46BA-8993-D74383FDFC4C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.7554/eLife.21673}}
