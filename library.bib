@article{Selverston:2009dp,
author = {Selverston, Allen I and Sz{\"u}cs, Attila and Huerta, Ramon and Pinto, Reynaldo and Reyes, Marcelo},
title = {{Neural mechanisms underlying the generation of the lobster gastric mill motor pattern.}},
journal = {Frontiers in neural circuits},
