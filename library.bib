month = mar,
publisher = {National Acad Sciences},
affiliation = {Faculty of Economics and Queens' College, University of Cambridge, Cambridge CB3 9ET, United Kingdom; Collegio Carlo Alberto, Universit{\`a} di Torino, 10024 Moncalieri, Italy; and edo@econ.cam.ac.uk chang.yan@cs.ox.ac.uk.},
keywords = {cooperation, Humans, information, networks, sociodynamics, sociology},
