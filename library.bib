@article{Pellegrino:2009ka,
author = {Pellegrino, Maurizio and Nakagawa, Takao},
title = {{Smelling the difference: controversial ideas in insect olfaction.}},
journal = {The Journal of experimental biology},
year = {2009},
volume = {212},
number = {Pt 13},
pages = {1973--1979},
month = jul,
affiliation = {Laboratory of Neurogenetics and Behavior, The Rockefeller University, New York, NY 10065, USA.},
keywords = {olfaction},
