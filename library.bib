year = {2008},
volume = {34},
number = {7},
pages = {837--853},
month = jul,
publisher = {Springer-Verlag},
affiliation = {ARL Division of Neurobiology, University of Arizona, Tucson, AZ 857210-0077, USA. jeffr@neurobio.arizona.edu},
keywords = {olfaction},
doi = {10.1007/s10886-008-9490-7},
