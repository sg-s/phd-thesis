journal = {Proceedings of the National Academy of {\ldots}},
year = {2012},
volume = {109},
number = {14},
pages = {5481--5486},
month = apr,
affiliation = {Stowers Institute for Medical Research, Kansas City, MO 64110, USA. lim@stowers.org},
keywords = {mouse, olfaction},
