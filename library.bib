@article{Wang:2012ew,
author = {Wang, Jing and Suri, Siddharth and Watts, Duncan J},
title = {{Cooperation and assortativity with dynamic partner updating.}},
journal = {Proceedings of the National Academy of {\ldots}},
year = {2012},
volume = {109},
number = {36},
pages = {14363--14368},
month = sep,
affiliation = {Department of Information, Operations and Management Sciences, Leonard N Stern School of Business, New York University, New York, NY 10012, USA. jwang5@stern.nyu.edu},
keywords = {cooperation, sociodynamics, sociology},
doi = {10.1073/pnas.1120867109},
pmid = {22904193},
