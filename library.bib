journal = {Journal of The Royal Society Interface},
year = {2012},
volume = {10},
number = {80},
pages = {20121013--20121013},
month = dec,
publisher = {The Royal Society},
affiliation = {Institute of Neuroinformatics, University of Zurich and ETH Zurich, Winterthurerstrasse 190, 8057 Zurich, Switzerland. jan@ini.phys.ethz.ch},
keywords = {CPG, Drosophila, flight, Locomotion},
doi = {10.1098/rsif.2012.1013},
