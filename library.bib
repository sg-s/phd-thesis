number = {1},
pages = {77--95},
month = aug,
affiliation = {Wallace H. Coulter Department of Biomedical Engineering, Georgia Institute of Technology/Emory University, Atlanta, GA 30332, USA. wafa.soofi@gatech.edu},
keywords = {cite-for-zoidberg, crab, lobster, network, STG},
doi = {10.1007/s10827-011-0375-3},
pmid = {22134522},
