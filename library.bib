file = {{Journal of Neuroscience 2013 Stierle.pdf:/Users/sigbhu/Dropbox/Library.papers3/Articles/2013/Stierle/Journal of Neuroscience 2013 Stierle.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1523/JNEUROSCI.5838-12.2013}}
}

