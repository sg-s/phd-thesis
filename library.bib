@article{Duistermars:2009du,
author = {Duistermars, Brian J and Chow, Dawnis M and Frye, Mark A},
title = {{Flies require bilateral sensory input to track odor gradients in flight.}},
journal = {Current biology : CB},
year = {2009},
volume = {19},
number = {15},
pages = {1301--1307},
month = aug,
affiliation = {Department of Physiological Science, University of California, Los Angeles, Los Angeles, CA 90095, USA.},
keywords = {olfaction},
doi = {10.1016/j.cub.2009.06.022},
pmid = {19576769},
