@article{Berck:2016cx,
author = {Berck, Matthew E and Khandelwal, Avinash and Claus, Lindsey and Hernandez-Nunez, Luis and Si, Guangwei and Tabone, Christopher J and Li, Feng and Truman, James W and Fetter, Rick D and Louis, Matthieu and Samuel, Aravinthan D T and Cardona, Albert and Calabrese, Ronald L},
title = {{The wiring diagram of a glomerular olfactory system}},
journal = {eLife},
year = {2016},
volume = {5},
pages = {e14859},
