@article{Hellmann:2015vz,
author = {Hellmann, Frank and Schultz, Paul and Grabow, Carsten and Heitzig, Jobst and Kurths, J{\"u}rgen},
title = {{Survivability: A Unifiying Concept for the Transient Resilience of Deterministic Dynamical Systems}},
journal = {arXiv.org},
