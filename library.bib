@article{Posfai:2016cf,
author = {P{\'o}sfai, M{\'a}rton and Gao, Jianxi and Cornelius, Sean P and Barab{\'a}si, Albert-L{\'a}szl{\'o} and D'Souza, Raissa M},
title = {{Controllability of multiplex, multi-time-scale networks}},
